{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "title": "Notice",
  "name": "zolo/notice",
  "category": "zoloblocks",
  "keywords": [
    "alert",
    "notice",
    "message",
    "warning",
    "info"
  ],
  "description": "Create custom-styled notices for key site messages",
  "apiVersion": 3,
  "textdomain": "zoloblocks",
  "example": {
    "attributes": {
      "preview": true
    },
    "viewportWidth": 400
  },
  "supports": {
    "anchor": false,
    "customClassName": false,
    "align": [
      "wide",
      "full"
    ]
  },
  "editorScript": "file:./index.js",
  "style": "file:./style-index.css",
  "viewScript": "file:./frontend.js"
}